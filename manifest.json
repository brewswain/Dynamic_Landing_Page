{
  "name": "Productivity Landing Page",
  "version": "1.0.0",
  "description": "Custom New Tab Page that includes weather report and to-do list!",
  "manifest_version": 2,
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "geolocation"]
}
